{
  "name": "cestino",
  "title": "cestino",
  "description": "A basic shopping cart implementation in javascript.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rekrut/cestino.git"
  },
  "scripts": {
    "test": "node_modules/karma/bin/karma start --singleRun && node_modules/.bin/jasmine --config='jasmine-node.json' && node_modules/karma/bin/karma start karma.conf.browserify.js --singleRun && node_modules/karma/bin/karma start karma.conf.global.js --singleRun",
    "document": "node_modules/jsdoc-to-markdown/bin/cli.js --no-cache --separators --private src/*.js > api.md",
    "build": "node scripts/build.js"
  },
  "directories": {
    "test": "test"
  },
  "main": "./src/Cart.js",
  "version": "2.1.1",
  "dependencies": {
    "atomicjs": "^4.4.0"
  },
  "devDependencies": {
    "browserify": "^14.5.0",
    "jasmine": "^2.99.0",
    "jasmine-core": "^2.99.1",
    "jsdoc-to-markdown": "^5.0.3",
    "jsdom": "^9.9.1",
    "karma": "^4.4.1",
    "karma-browserify": "^5.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-jsdom-launcher": "^5.0.0",
    "karma-requirejs": "^1.1.0",
    "requirejs": "^2.3.6",
    "uglify-js": "^3.7.2",
    "watchify": "^3.11.1"
  },
  "keywords": [
    "shopping",
    "cart"
  ],
  "author": "Daniel Moritz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rekrut/cestino/issues"
  },
  "homepage": "https://github.com/rekrut/cestino#readme"
}
